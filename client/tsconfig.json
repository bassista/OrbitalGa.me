{
  "compilerOptions": {
    "noEmitOnError": false,
    "experimentalDecorators": true,
    "allowUnreachableCode": true,
    "strict": true,
    "target": "es2015",
    "lib": [
      "esnext",
      "dom",
      "es2017",
      "esnext.asynciterable"
    ],
    "noImplicitAny": true,
    "module": "commonjs",
    "inlineSourceMap": true,
    "strictNullChecks": false,
    "pretty": true,
    "noEmit": true,
    "moduleResolution": "node",
    "strictPropertyInitialization": false,
    "allowSyntheticDefaultImports": true,
    "skipLibCheck": true,
    "baseUrl": "./",
    "jsx": "react",
    "paths": {
      "@common/*": [
        "../common/*"
      ]
    }
  },
  "exclude": [
    "node_modules",
    "dist"
  ]
}
