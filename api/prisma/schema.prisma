datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

generator client {
  provider = "prisma-client-js"
}

model User {
  id        Int      @default(autoincrement()) @id
  createdAt DateTime @default(now())
  username  String
  password  String
}

model ServerLeaderboardEntry {
  id        Int      @default(autoincrement()) @id
  createdAt DateTime @default(now())
}

model GlobalLeaderboardEntry {
  id        Int      @default(autoincrement()) @id
  createdAt DateTime @default(now())
  username  String
  password  String
}

model LeaderboardEntry {l
  id                   Int @default(autoincrement()) @id
  aliveTime            Int
  calculatedScore      Int
  damageGiven          Int
  damageTaken          Int
  enemiesKilled        Int
  eventsParticipatedIn Int
  shotsFired           Int
}